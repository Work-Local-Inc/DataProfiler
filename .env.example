# Copy this file to .env and fill in your values
# In Replit, use Secrets tab instead of .env file

# Database (Required)
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/dataprofiler

# Redis (Optional - for caching)
REDIS_URL=redis://localhost:6379

# API Management
API_RATE_LIMIT=100
MONTHLY_BUDGET=500

# SEO & Technology APIs
BUILTWITH_API_KEY=
DATAFORSEO_LOGIN=
DATAFORSEO_PASSWORD=

# Social Media APIs
TWITTER_API_KEY=
RAPIDAPI_KEY=
TIKTOK_API_HOST=tiktok-api23.p.rapidapi.com

# Google APIs
GOOGLE_PLACES_API_KEY=
GOOGLE_PAGESPEED_API_KEY=

# Facebook/Instagram OAuth
FACEBOOK_APP_ID=
FACEBOOK_APP_SECRET=
INSTAGRAM_CLIENT_ID=
INSTAGRAM_CLIENT_SECRET=

# Google OAuth
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# Security
JWT_SECRET=generate_random_32_character_string_here
SESSION_SECRET=another_random_string_here

# Server
PORT=3000
NODE_ENV=development
API_URL=http://localhost:3000

# Email (Optional)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=
SMTP_PASS=

# Monitoring (Optional)
SENTRY_DSN=